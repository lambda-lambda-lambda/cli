{
  "openapi": "3.0.0",
  "info": {
    "title": "{{pkgName}}",
    "version": "0.0.1"
  },
  "servers": [
    {
      "url": "{AppUrl}",
      "variables": {
        "AppUrl": {
          "default": "http://localhost:3000"
        }
      }
    }
  ],
  "paths": {
    "{{routePath}}": {
      "get": {
        "description": "Route description.",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/html": {
                "schema": {
                  "type": "string"
                }
              }
            },
            "headers": {
              "Content-Type": {
                "schema": {
                  "type": "string",
                  "example": "text/html"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {},
  "tags": []
}
